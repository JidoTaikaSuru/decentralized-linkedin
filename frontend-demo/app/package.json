{
  "name": "@xmtp/react-quickstart-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "yarn typecheck && vite build",
    "clean:dist": "rm -rf react-quickstart/dist",
    "clean": "rm -rf .turbo && rm -rf react-quickstart/node_modules && yarn clean:dist",
    "lint": "eslint react-quickstart --ignore-path ../../.gitignore",
    "format:base": "prettier --ignore-path ../.gitignore",
    "format:check": "yarn format:base -c react-quickstart",
    "format": "yarn format:base -w react-quickstart",
    "quickstart": "yarn dev",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@rainbow-me/rainbowkit": "^1.0.11",
    "@xmtp/content-type-remote-attachment": "^1.1.2",
    "@xmtp/react-components": "workspace:*",
    "@xmtp/react-sdk": "^2.1.0",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "viem": "^1.13.0",
    "wagmi": "^1.4.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.8",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.50.0",
    "postcss": "^8.4.30",
    "postcss-preset-env": "^9.1.4",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
